mixin video(video)
    a(href=`/videos/${video.id}` class="video-link")
        div(class="thumbnail")
            //- img(src=video.thumbnail)
        div(class="video-info")
            h1=video.title
            //- p=video.description
            ul
                each hashtag in video.hashtags
                    li=hashtag
            //- small=video.createdAt